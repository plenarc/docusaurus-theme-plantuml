{
  "name": "docusaurus-theme-plantuml",
  "version": "1.0.1",
  "description": "PlantUML components for Docusaurus. Renders PlantUML diagrams in markdown using a PlantUML server.",
  "author": "plenarc",
  "license": "MIT",
  "keywords": [
    "docusaurus",
    "plugin",
    "plantuml",
    "diagram",
    "documentation",
    "markdown",
    "uml"
  ],
  "homepage": "https://github.com/plenarc/docusaurus-theme-plantuml/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plenarc/docusaurus-theme-plantuml.git"
  },
  "scripts": {
    "format": "npx @biomejs/biome format --write ./src",
    "lint": "npx @biomejs/biome lint ./src",
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "prepare": "tsc --build",
    "test": "node --test",
    "test:watch": "node --test --watch"
  },
  "main": "lib/index.js",
  "types": "src/theme-plantuml.d.ts",
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "exports": {
    "./lib/*": "./lib/*",
    "./src/*": "./src/*",
    ".": {
      "types": "./src/theme-plantuml.d.ts",
      "default": "./lib/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/module-type-aliases": "^3.9.2",
    "@docusaurus/theme-common": "^3.9.2",
    "@docusaurus/types": "3.9.2",
    "joi": "^18.0.1",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@docusaurus/tsconfig": "^3.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "engines": {
    "node": ">=22.0"
  },
  "packageManager": "pnpm@10.18.3"
}
