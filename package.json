{
  "name": "docusaurus-theme-plantuml",
  "version": "0.2.5",
  "description": "PlantUML components for Docusaurus. Renders PlantUML diagrams in markdown using a PlantUML server.",
  "author": "plenarc",
  "license": "MIT",
  "keywords": [
    "docusaurus",
    "plugin",
    "plantuml",
    "diagram",
    "documentation",
    "markdown",
    "uml"
  ],
  "homepage": "https://github.com/plenarc/docusaurus-theme-plantuml/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plenarc/docusaurus-theme-plantuml.git"
  },
  "scripts": {
    "format": "npx @biomejs/biome format --write ./src",
    "lint": "npx @biomejs/biome lint ./src",
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "prepare": "tsc --build",
    "test": "node --test",
    "test:watch": "node --test --watch"
  },
  "main": "lib/index.js",
  "types": "src/theme-plantuml.d.ts",
  "files": ["lib", "README.md", "LICENSE"],
  "sideEffects": false,
  "exports": {
    "./lib/*": "./lib/*",
    "./src/*": "./src/*",
    ".": {
      "types": "./src/theme-plantuml.d.ts",
      "default": "./lib/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/module-type-aliases": "^3.7.0",
    "@docusaurus/theme-common": "^3.7.0",
    "@docusaurus/types": "3.7.0",
    "joi": "^17.13.3",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@docusaurus/tsconfig": "^3.7.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "engines": {
    "node": ">=22.0"
  },
  "packageManager": "pnpm@10.2.0+sha512.0d27364e0139c6aadeed65ada153135e0ca96c8da42123bd50047f961339dc7a758fc2e944b428f52be570d1bd3372455c1c65fa2e7aa0bfbf931190f9552001"
}
